{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-undef":0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "req|res|next|val" }]
  }
}
