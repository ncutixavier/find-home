{
  "name": "find-home",
  "version": "1.0.0",
  "description": "Find Home Project",
  "main": "src/app.js",
  "scripts": {
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo && npm run migrate",
    "seeding": "npx sequelize-cli db:seed:all",
    "migrateDB": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "migrateTest": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seeding",
    "build": "babel src -d dist",
    "start": "npm run migrateDB && npm run build && node ./dist/server.js",
    "dev": "nodemon --exec babel-node ./src/server.js",
    "test": "NODE_ENV=test npm run migrateTest && NODE_ENV=test nyc mocha --recursive --require @babel/polyfill --require @babel/register ./src/test/**.js --timeout 200000 --exit",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncutixavier/find-home.git"
  },
  "keywords": [
    "home"
  ],
  "author": "ncutixxavier",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ncutixavier/find-home/issues"
  },
  "homepage": "https://github.com/ncutixavier/find-home#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "swagger-jsdoc": "^4.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "i18n": "^0.13.2",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.13",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "swagger-ui-express": "^4.1.4"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  }
}
